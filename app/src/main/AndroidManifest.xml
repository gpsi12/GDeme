<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gpsi.gdeme">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".GlobalApp"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.TextPageAct"></activity>
        <activity android:name=".ui.LoginAct"></activity>
        <activity android:name=".ui.PhotoAlbumAct"></activity>
        <activity android:name=".ui.RegistrySettingAct"></activity>
        <activity android:name=".ui.RegisterprotocolAct"></activity>
    </application>

</manifest>